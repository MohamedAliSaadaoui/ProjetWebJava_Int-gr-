{% block javascripts %}
{{ parent() }}
<script>
    document.addEventListener("DOMContentLoaded", function() {
        // Contact seller functionality
        const showSellerContactBtn = document.getElementById('showSellerContact');
        const sellerContactInfo = document.getElementById('sellerContactInfo');
        
        if (showSellerContactBtn && sellerContactInfo) {
            showSellerContactBtn.addEventListener('click', function(event) {
                // Prevent default button behavior
                event.preventDefault();
                
                if (sellerContactInfo.style.display === 'none') {
                    sellerContactInfo.style.display = 'block';
                    showSellerContactBtn.innerHTML = '<i class="icofont-close"></i> Hide Contact Info';
                } else {
                    sellerContactInfo.style.display = 'none';
                    showSellerContactBtn.innerHTML = '<i class="icofont-ui-chat"></i> Contact Seller';
                }
            });
        }
        
        // Favorites functionality
        const addToFavoritesBtn = document.getElementById('addToFavorites');
        const favoriteIcon = document.getElementById('favoriteIcon');
        
        if (addToFavoritesBtn) {
            // Check if product is already in favorites
            const productId = addToFavoritesBtn.getAttribute('data-id');
            
            fetch(`/product/favorite/check/${productId}`)
                .then(response => response.json())
                .then(data => {
                    if (data.isInFavorites) {
                        favoriteIcon.classList.remove('far');
                        favoriteIcon.classList.add('fas');
                        favoriteIcon.style.color = 'red';
                        addToFavoritesBtn.textContent = ' Remove from Favorites';
                        addToFavoritesBtn.prepend(favoriteIcon);
                    }
                });
            
            // Add click event listener for favorites button
            addToFavoritesBtn.addEventListener('click', function(event) {
                event.preventDefault();
                
                fetch(`/product/favorite/${productId}`, {
                    method: 'POST',
                    headers: {
                        'X-Requested-With': 'XMLHttpRequest'
                    }
                })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        if (data.isInFavorites) {
                            // Redirect to favorites page after adding to favorites
                            window.location.href = '/favorites';
                        } else {
                            favoriteIcon.classList.remove('fas');
                            favoriteIcon.classList.add('far');
                            favoriteIcon.style.color = '';
                            addToFavoritesBtn.textContent = ' Add to Favorites';
                            addToFavoritesBtn.prepend(favoriteIcon);
                            alert(data.message);
                        }
                    } else {
                        // User not logged in or other error
                        alert(data.message);
                        window.location.href = '/login';
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    alert('Failed to update favorites. Please try again.');
                });
            });
        }
    });
</script>
