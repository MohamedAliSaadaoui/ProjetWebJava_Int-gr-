    <div class="gs-blog-wrapper">
        <div class="container">
            <div class="row flex-column-reverse flex-lg-row">
                <!-- Sidebar avec recherche -->
                <div class="col-12 col-lg-4 mt-40 mt-lg-0">
                    <div class="gs-blog-sidebar-wrapper">
                        <div class="single-blog-widget">
                            <h5 class="widget-title">Recherche</h5>
                            <form class="search-form" action="{{ path('app_question_index') }}" method="GET">
                                <input class="input-box" type="text" name="search"
                                       value="{{ app.request.query.get('search') }}"
                                       placeholder="Rechercher une question...">
                                <button type="submit">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                        <path d="M21 21L16.65 16.65M19 11C19 15.4183 15.4183 19 11 19C6.58172 19 3 15.4183 3 11C3 6.58172 6.58172 3 11 3C15.4183 3 19 6.58172 19 11Z" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                                    </svg>
                                </button>
                            </form>
                        </div>
                    </div>
                </div>

                <!-- Liste des questions -->
                <div class="col-12 col-lg-8">
                    <div class="gs-blog-content-wrapper">
                        <h2 class="mb-4">Liste des Questions</h2>

                        {% if questions is empty %}
                            <div class="alert alert-info">
                                Aucune question n'a été trouvée.
                                <a href="{{ path('app_question_new') }}" class="btn btn-primary btn-sm ml-2">
                                    <i class="fas fa-plus"></i> Ajouter une question
                                </a>
                            </div>
                        {% else %}
                            <a href="{{ path('app_question_new') }}" class="btn btn-primary mb-4">
                                <i class="fas fa-plus"></i> Nouvelle question
                            </a>

                            <div class="row">
                                {% for question in questions %}
                                    <div class="col-12 mb-4">
                                        <div class="card shadow-sm">
                                            <div class="card-body">
                                                <h5 class="card-title">
                                                    <a href="{{ path('app_question_show', {'idQ': question.idQ}) }}" class="text-decoration-none text-dark">
                                                        {{ question.questionText|length > 100 ? question.questionText|slice(0, 100) ~ '...' : question.questionText }}
                                                    </a>
                                                </h5>
                                                <div class="card-text mb-2">
                                                    <div class="d-flex justify-content-between">
                                        <span class="text-muted">
                                            <i class="far fa-user"></i> {{ question.name }}
                                        </span>
                                                        <span class="text-muted">
                                            <i class="far fa-calendar-alt"></i> {{ question.createdAt|date('d/m/Y H:i') }}
                                        </span>
                                                    </div>
                                                </div>
                                                <div class="d-flex justify-content-between align-items-center">
                                                    <div class="btn-group">
                                                        <button class="btn btn-sm btn-outline-primary btn-repondre" data-question-id="{{ question.idQ }}">
                                                            <i class="fas fa-comment"></i> Répondre
                                                        </button>
                                                        <button class="btn btn-sm btn-outline-info toggle-feedbacks" data-question-id="{{ question.idQ }}">
                                                            <i class="fas fa-comments"></i> Afficher les réponses
                                                        </button>
                                                    </div>
                                                    <small class="text-muted">
                                                        <i class="far fa-comment"></i> {{ question.feedbacks|length }} réponse(s)
                                                    </small>
                                                </div>

                                                <!-- Section des feedbacks -->
                                                <div id="feedbacks-{{ question.idQ }}" class="mt-3" style="display: none;">
                                                    <h6>Réponses :</h6>
                                                    {% for feedback in question.feedbacks %}
                                                        <div class="answer-card
                                                             {% if feedback.approved > 10 %}approved-high
                                                             {% elseif feedback.approved >= 0 %}approved-medium
                                                             {% else %}approved-low
                                                             {% endif %}">
                                                            <div class="card-body">
                                                                <p class="card-text">{{ feedback.feedbackText }}</p>
                                                                <p class="card-text">
                                                                    <small class="text-muted">
                                                                        Répondu par {{ feedback.userName }} le {{ feedback.answeredAt|date('d/m/Y H:i') }}
                                                                    </small>
                                                                </p>
                                                                <div class="btn-group">
                                                                    <button class="btn btn-vote upvote" data-feedback-id="{{ feedback.idF }}" data-action="up">
                                                                        👍 <span class="approve-count">{{ feedback.approved }}</span>
                                                                    </button>
                                                                    <button class="btn btn-vote downvote" data-feedback-id="{{ feedback.idF }}" data-action="down">
                                                                        👎
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    {% else %}
                                                        <div class="alert alert-info">
                                                            Aucune réponse pour cette question.
                                                        </div>
                                                    {% endfor %}
                                                </div>

                                                <!-- Token CSRF pour les requêtes AJAX -->
                                                <meta name="csrf-token" content="{{ csrf_token('vote') }}">

                                                <!-- Formulaire d'ajout de réponse -->
                                                <div id="reponse-card-{{ question.idQ }}" class="mt-3" style="display: none;">
                                                    <div class="card">
                                                        <div class="card-body">
                                                            <h6>Ajouter une réponse :</h6>
                                                            <form action="{{ path('app_feedback_new', {'idQ': question.idQ}) }}" method="POST" class="form-reponse" data-question-id="{{ question.idQ }}">
                                                                <div class="form-group">
                                                                    <textarea name="feedback_text" class="form-control" rows="3" placeholder="Votre réponse..." required></textarea>
                                                                </div>
                                                                <div class="form-group">
                                                                    <input type="text" name="user_name" class="form-control" placeholder="Votre nom" required>
                                                                </div>
                                                                <button type="submit" class="btn btn-primary btn-sm">
                                                                    <i class="fas fa-paper-plane"></i> Envoyer
                                                                </button>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                {% endfor %}
                            </div>
                        {% endif %}
                    </div>
                </div>

                <!-- Notification -->
                <div id="notification-card" class="notification-card" style="display: none;">
                    <div class="notification-content">
                        <span class="notification-message"></span>
                        <button class="notification-close-btn">&times;</button>
                    </div>
                </div>
        </div>
    </div>
