{% extends 'base.html.twig' %}

{% block title %}Réclamation index{% endblock %}

{% block body %}
    <h1>Réclamation index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for r_clamation in r_clamations %}
            <tr>
                <td>{{ r_clamation.id }}</td>
                <td>
                    <a href="{{ path('app_r_clamation_crud_show', {'id': r_clamation.id}) }}">show</a>
                    <a href="{{ path('app_r_clamation_crud_edit', {'id': r_clamation.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_r_clamation_crud_new') }}">Create new</a>
{% endblock %}
